{
  "name": "chomp-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@latest",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && cp _headers ./dist/_headers && cp _redirects ./dist/_redirects",
    "build:analyze": "ANALYZE=true next build && cp _headers ./dist/_headers && cp _redirects ./dist/_redirects",
    "build:production": "NODE_ENV=production next build && cp _headers ./dist/_headers && cp _redirects ./dist/_redirects",
    "build:modern": "NODE_ENV=production MODERN_BUILD=true next build && cp _headers ./dist/_headers && cp _redirects ./dist/_redirects",
    "start": "next start",
    "serve": "bunx serve -p 3000 -s ./dist",
    "serve:prod": "bun run build && bunx serve -p 3000 -s ./dist",
    "export": "next build && cp _headers ./dist/_headers && cp _redirects ./dist/_redirects",
    "lint": "oxlint src --format=unix && oxlint app --format=unix && next lint",
    "lint:fix": "oxlint src --fix && oxlint app --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' 'app/**/*.{ts,tsx,js,jsx}' '*.{ts,tsx,js,jsx,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx}' 'app/**/*.{ts,tsx,js,jsx}' '*.{ts,tsx,js,jsx,json,md}'",
    "check:all": "bun run lint && bun run format",
    "bundle:size": "bun scripts/bundle-size.js",
    "bundle:analyze": "bun scripts/bundle-analyze.js",
    "verify:sourcemaps": "bun scripts/verify-sourcemaps.js",
    "optimize:build": "bun run build:modern && bun run bundle:analyze",
    "build:verify": "bun run build && bun run verify:sourcemaps",
    "performance:check": "bun scripts/performance-check.js",
    "lighthouse:audit": "lhci autorun",
    "lighthouse:build-and-audit": "bun run build && bun run lighthouse:audit",
    "pre-commit": "uv tool run pre-commit run --all-files",
    "pre-commit:install": "uv tool run pre-commit install && uv tool run pre-commit install --hook-type pre-push",
    "type-check": "tsc --noEmit --project .",
    "predev": "bun run lint",
    "prebuild": "bun run lint && bun run type-check"
  },
  "dependencies": {
    "@mui/icons-material": "7.1.1",
    "@mui/material": "7.1.1",
    "@mui/x-charts": "8.5.2",
    "@mysten/dapp-kit": "0.16.13",
    "@mysten/sui": "1.34.0",
    "@next/mdx": "15.3.3",
    "@solana/react": "2.1.1",
    "@solana/wallet-adapter-backpack": "0.1.14",
    "@solana/wallet-adapter-base": "0.9.27",
    "@solana/wallet-adapter-coinbase": "0.1.23",
    "@solana/wallet-adapter-ledger": "0.9.29",
    "@solana/wallet-adapter-phantom": "0.9.28",
    "@solana/wallet-adapter-react": "0.15.39",
    "@solana/wallet-adapter-react-ui": "0.9.39",
    "@solana/wallet-adapter-solflare": "0.6.32",
    "@solana/wallet-adapter-torus": "0.11.32",
    "@solana/wallet-adapter-wallets": "0.19.37",
    "@solana/web3.js": "1.98.2",
    "@tanstack/react-query": "5.80.7",
    "asciimath2ml": "1.0.7",
    "chart.js": "4.5.0",
    "immer": "10.1.1",
    "next": "15.3.3",
    "react": "19.1.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "ua-parser-js": "2.0.3",
    "viem": "2.31.3",
    "wagmi": "2.15.6",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@types/node": "20.19.1",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@types/ua-parser-js": "0.7.39",
    "bun": "1.2.16",
    "eslint": "9.29.0",
    "eslint-config-next": "15.3.3",
    "oxlint": "1.1.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
